<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>AddProduct</servlet-name>
    <servlet-class>cn.com.leadar.AddProduct</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListProduct</servlet-name>
    <servlet-class>cn.com.leadar.ListProduct</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>AddProduct</servlet-name>
    <url-pattern>/servlet/AddProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListProduct</servlet-name>
    <url-pattern>/servlet/ListProduct</url-pattern>
  </servlet-mapping>



 

</web-app>